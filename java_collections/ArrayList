package cse.edu.bd.bubt;
import java.util.*;

public class ListExample {
    public static void main(String[] args) {
        List<String> studentList = new ArrayList<>();

        // উপাদান যোগ করা
        studentList.add("Rahim");
        studentList.add("Karim");
        studentList.add("Rahim"); // ডুপ্লিকেটও রাখা যাবে

        // উপাদান দেখানো
        System.out.println("Student List:");
        for (String name : studentList) {
            System.out.println(name);
        }

        // নির্দিষ্ট ইনডেক্স থেকে উপাদান পাওয়া
        System.out.println("First student: " + studentList.get(0));

        // উপাদান মোছা
        studentList.remove("Karim");

        System.out.println("Updated List: " + studentList);
    }

}
